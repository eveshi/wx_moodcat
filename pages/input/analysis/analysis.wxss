.bgLine{
  position: absolute; 
  margin-top: 16vh;
  width: 40vw;
  height: 150vh;
  border-right: 1vw solid #72523d;
  z-index: 0; 
}

.mainpage{
  position: absolute;
  margin-top: 19vh;
  z-index: 1; 
} 

.headPic{
  position: absolute;
  margin-top: 3vh;
  margin-left: 30vw;
  height: 16vh;
  width: 40vw;
}

.description{
  width: 75vw;
  margin: auto auto auto 20vw;
  text-align: center;
  font-size: 14px;
  color: #FEFEFD;
  border: 2px solid #72523d;
  border-radius: 5px;
  background-color: #72523d;
  display: flex;
  flex-direction: row;
  box-shadow: 2px 3px 4px rgba(0, 0, 0, .5);
}

.desText{
  margin: auto 0 auto auto;
}

.quesMark{
  margin: auto auto auto 2px;
  height: 16px;
  width: 16px;
}

.statement{
  margin: 5vw;
  padding: 20rpx; 
  height: 50vh;
  border-left: 5px solid #8A976A;
  background-color: #E6E7CE;
  box-shadow: 2px 3px 4px rgba(0, 0, 0, .5);
  border-radius: 10rpx;
}

.inputArea{
  margin-top: 15rpx;
  padding: 20rpx;
  height: 15vh;
  width: 78vw;
  color: #8A976A;
  background-color: #FEFEFD;
  border-radius: 7rpx; 
}

.trapTitle{
  margin-top: 20rpx;
  text-align: center;
}

.trapPicker{
  text-align: center;
}

.next{
  width: 300rpx;
  height: 50px;
  text-align: center;
  border: 1px solid #FEFEFD;
  border-radius: 10px;
  color: #FEFEFD;
  background-color: #8A976A;
}

